	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="AK_Weapon_Angelina"]/tools</xpath>
		<value>
			<tools Inherit = "false">
				<li Class="CombatExtended.ToolCE">
					<label>handle</label>
					<capacities>
						<li>Poke</li>
					</capacities>
					<power>4</power>
